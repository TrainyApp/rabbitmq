{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": ["config:recommended"],
    "enabledManagers": ["docker"],
    "customManagers": [
      {
        "customType": "regex",
        "description": "Update rabbitmq-delayed-message-exchange version",
        "fileMatch": ["Dockerfile"],
        "matchStrings": ["https:\\/\\/github\\.com\\/rabbitmq\\/rabbitmq-delayed-message-exchange\\/releases\\/download\\/v(?<version>[0-9.]+)\\/rabbitmq_delayed_message_exchange-\\k<version>\\.ez"],
        "datasourceTemplate": "github-releases",
        "depNameTemplate": "rabbitmq/rabbitmq-delayed-message-exchange",
        "versioningTemplate": "regex:^?(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)$"
      }
    ]
  }